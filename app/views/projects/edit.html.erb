
<%=form_for @project do |f| %>

  <div class="form-group">
    <label>titre du projet</label>
    <%= f.text_field :title %>
  </div>

  <div class="form-group">
    <label>Lieu</label>
    <%= f.text_field :localization %>
  </div>

  <div class="form-group">
    <label>note</label>
    <%= f.text_field :comment %>
  </div>


  <div class="form-group">
    <button type='submit'>Modifier le projet</button>
  </div>



 <%end%>