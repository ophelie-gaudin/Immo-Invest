<%= form_tag url_for(action: 'create'), method: "post" do |f| %>

  <%= label_tag "Titre du projet" %>
  <%= text_field_tag 'title', @project.title %>
  <br /> <br />


  <%= label_tag "Lieu" %>
  <%= text_field_tag 'localization', @project.localization %>
  <br /> <br />




  <%= label_tag "Description du projet" %>
  <%= text_area_tag 'comment', @project.comment%>
  <br /> <br />


  <%= link_to "Creer votre projet",projects_path, method: :POST%>



<% end %>